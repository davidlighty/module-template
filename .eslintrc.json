{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier", "plugin:flowtype/recommended"],
  "env": {
    "browser": true
  },
  "plugins": ["jest", "flowtype", "prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "no-underscore-dangle": ["error", { "allow": ["__get__"] }],
    "linebreak-style": 0,
    "no-default-export": true
  }
}
